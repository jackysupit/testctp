        <record id="sequence_budget_number" model="ir.sequence">
            <field name="name">Budget number sequence</field>
            <field name="code">crossovered.budget</field>
            <field name="prefix">BGT/%(range_year)s/9</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">4</field>
        </record>

